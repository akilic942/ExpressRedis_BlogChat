  <% layout('layout') -%>
<% if (!data.response) { %>

      <% data.forEach(function(obj) { %>
        <% if (obj !== null) { %>

        <div class="well">
          <ul>
            <div class="page-header">
              <a href="/blog/<%=obj.id%>"> <%=obj.Titel%> </a>


                <small>von <%= obj.Author %>
                </small>
              </h1>
            <small>PostID: <%=obj.id %>, Erstellt am  <%=obj.Erstellt %>
            </small>

               </div>
                <%=obj.POST %>
                <hr>

                </ul>
            </div>

            <% }; %>
      <% }); %>
<% } %>

<% if (data.response) { %>
  <div class="well">
    <ul>
      <div class="page-header">
   <p>404:</p> <%=data.response%>
      </div>
  <% } %>
